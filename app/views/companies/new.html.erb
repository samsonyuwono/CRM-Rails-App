<h2>New Company Form</h2>

<%= form_for (@company) do |f| %>
  <%= f.hidden_field :user_id %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br />

  <%= f.label :revenue, "Revenue(in 100,000):" %>
  <%= f.number_field :revenue %><br />


  Check this box for customer:<%= f.check_box :customer %><br>
  <%= f.submit "Create Company" %>
<% end %>

