<%= form_for :leads do |f| %>

  <%= f.label :name %>:
  <%= f.text_field :name %><br />

  <%= f.label :email %>:
  <%= f.text_field :email %><br />

  <%= f.label :phone_number %>:
  <%= f.number_field :phone_number %><br />

  Check this box for contact:<%= f.check_box :contact %><br />

  <%= f.submit %>

  <% end %>
